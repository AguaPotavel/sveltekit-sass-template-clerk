// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

enum UserAccess {
  REGULAR
  PRO
  BUSSINESS
  ADMIN
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String     @id
  userAccess UserAccess @default(REGULAR)
  createdAt  DateTime   @default(now())
}
